<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>ezpython</title>
  <% if (htmlWebpackPlugin.options.nodeModules) { %>
    <!-- Add `node_modules/` to global paths so `require` works properly in development -->
    <script>
      require('module').globalPaths.push('<%= htmlWebpackPlugin.options.nodeModules.replace(/\\/g, '\\\\') %>')
    </script>
    <% } %>
    <script src="lib/blockly/blockly_compressed.js"></script>
    <script src="lib/ezp/ezp_compressed.js"></script>
  </head>

<body>
  <div id="app"></div>
  <!-- Set `__static` path to static files in production -->
  <script>
    window.BlocklyProxy = window.Blockly
    if (typeof process !== 'undefined' && process.env.NODE_ENV !== 'development') window.__static = require('path').join(__dirname, '/static').replace(/\\/g, '\\\\')
  </script>
  <!-- webpack builds are automatically injected -->
</body>

</html>