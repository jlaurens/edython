{
  "name": "edython",
  "version": "0.3.0",
  "author": "jlaurens <jlaurens@edython.eu>",
  "description": "Python made easy with Brython",
  "license": "EUPL-1.2",
  "main": "./dist/electron/main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/jlaurens/edython.git"
  },
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "build:web_debug": "cross-env EYO_BUILD_MODE=debug BUILD_TARGET=web node .electron-vue/build.js",
    "build:full_debug": "cross-env BUILD_TARGET=full_debug node .electron-vue/build.js",
    "serve": "http-server dist/web",
    "dev": "node .electron-vue/dev-runner.js",
    "debug": "cross-env EYO_BUILD_MODE=debug node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:web": "cross-env NODE_ENV=production webpack --display-error-details --progress --colors --config .electron-vue/webpack.web.config.js",
    "pack:main": "cross-env NODE_ENV=production webpack --display-error-details --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --display-error-details --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix",
    "eyo:test": "mocha src/lib/eyo/parser/*.test.js --timeout 10000",
    "eyo:graminit": "python3 ./src/lib/eyo/Python/graminit.py",
    "eyo:t3": "python3 \"./src/lib/eyo/t3/python types.py\" && python3 \"./bin/helpers/install t3.py\"",
    "eyo:module": "python3 ./bin/helpers/modulebot.py --no-suffix functions stdtypes datastructures datamodel && python3 ./bin/helpers/modulebot.py string turtle math cmath decimal fractions random statistics datetime calendar collections collections.abc heapq  bisect array weakref types copy pprint reprlib enum",
    "eyo:prepare": "python3 ./bin/helpers/prepare.py",
    "eyo:update": "python3 ./bin/helpers/update.py",
    "eyo:build": "bash ./bin/build.sh"
  },
  "build": {
    "productName": "Edython",
    "appId": "eu.edython",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap-vue": "^2.15.0",
    "clean-css": "^4.2.3",
    "css-element-queries": "^1.2.3",
    "element-resize-detector": "^1.2.1",
    "file-saver": "^2.0.2",
    "fs": "0.0.1-security",
    "gsap": "^3.3.3",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "pako": "^1.0.11",
    "stack-trace": "0.0.10",
    "tippy": "0.0.0",
    "vue": "^2.6.11",
    "vue-drawer-layout": "^1.3.0",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.18.2",
    "vue-router": "^3.3.4",
    "vue-split-panel": "^1.0.4",
    "vuex": "^3.4.0",
    "vuex-persist": "^2.2.0",
    "xregexp": "^4.3.0"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.8.5",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "^9.0.4",
    "electron-builder": "^22.7.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.0.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "karma": "^5.1.0",
    "mocha": "^8.0.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "raw-loader": "^4.0.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vue-tippy": "^4.4.0",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
